    "configuration": {
      "title": "Cluster Pilot",
      "properties": {
        "clusterPilot.kubeconfigPath": {
          "type": "string",
          "default": "~/.kube/config",
          "description": "Path to your kubeconfig file"
        },
        "clusterPilot.refreshInterval": {
          "type": "number",
          "default": 5000,
          "minimum": 0,
          "description": "Auto-refresh interval in milliseconds (0 to disable)"
        },
        "clusterPilot.enableMetrics": {
          "type": "boolean",
          "default": true,
          "description": "Enable metrics collection (requires metrics-server)"
        },
        "clusterPilot.logLines": {
          "type": "number",
          "default": 1000,
          "minimum": 100,
          "maximum": 10000,
          "description": "Number of log lines to fetch"
        },
        "clusterPilot.theme": {
          "type": "string",
          "enum": [
            "auto",
            "light",
            "dark"
          ],
          "default": "auto",
          "description": "Theme for webviews"
        },
        "clusterPilot.health.cpuWarning": {
          "type": "number",
          "default": 70,
          "minimum": 1,
          "maximum": 100,
          "description": "CPU usage warning threshold (percentage)"
        },
        "clusterPilot.health.cpuCritical": {
          "type": "number",
          "default": 90,
          "minimum": 1,
          "maximum": 100,
          "description": "CPU usage critical threshold (percentage)"
        },
        "clusterPilot.health.memoryWarning": {
          "type": "number",
          "default": 80,
          "minimum": 1,
          "maximum": 100,
          "description": "Memory usage warning threshold (percentage)"
        },
        "clusterPilot.health.memoryCritical": {
          "type": "number",
          "default": 95,
          "minimum": 1,
          "maximum": 100,
          "description": "Memory usage critical threshold (percentage)"
        },
        "clusterPilot.health.restartWarning": {
          "type": "number",
          "default": 3,
          "minimum": 1,
          "description": "Pod restart warning threshold"
        },
        "clusterPilot.health.restartCritical": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "description": "Pod restart critical threshold"
        },
        "clusterPilot.backupDirectory": {
          "type": "string",
          "default": "~/.jas-cluster-pilot/backups",
          "description": "Directory to store cluster backups"
        },
        "clusterPilot.enableVerboseLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable verbose debug logging to output channel"
        },
        "clusterPilot.bypassProxyForLocalConnections": {
          "type": "boolean",
          "default": true,
          "description": "Bypass corporate proxy for local Kubernetes connections (localhost, Docker Desktop)"
        },
        "clusterPilot.debounceMs": {
          "type": "number",
          "default": 300,
          "minimum": 0,
          "maximum": 5000,
          "description": "Debounce delay in milliseconds for tree refresh operations (0-5000ms, default: 300ms)"
        },
        "clusterPilot.progressiveLoadingPageSize": {
          "type": "number",
          "default": 100,
          "minimum": 10,
          "maximum": 1000,
          "description": "Number of items to load per page in tree views (10-1000, default: 100)"
        },
        "clusterPilot.enableTelemetry": {
          "type": "boolean",
          "default": true,
          "description": "Enable telemetry for cache hit rates and performance metrics"
        }
      }
    }
