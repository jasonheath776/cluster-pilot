  "description": "Kubernetes Lens-style cluster management for VS Code",
  "version": "0.1.0",
  "publisher": "jheath",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonheath776/cluster-pilot.git"
  },
  "bugs": {
    "url": "https://github.com/jasonheath776/cluster-pilot/issues"
  },
  "homepage": "https://github.com/jasonheath776/cluster-pilot#readme",
  "license": "MIT",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "kubernetes",
    "k8s",
    "kubectl",
    "docker",
    "containers",
    "devops"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onView:clusterPilot.clusters",
    "onView:clusterPilot.workloads",
    "onView:clusterPilot.config",
    "onView:clusterPilot.network",
    "onView:clusterPilot.storage",
    "onCommand:clusterPilot.openKubectlTerminal",
    "onStartupFinished"
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
